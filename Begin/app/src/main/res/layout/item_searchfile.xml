<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="80dp">

    <TextView
            android:text="测试文件"
            android:layout_width="249dp"
            android:layout_height="53dp" android:id="@+id/tv_filename"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="@+id/guideline3"
            app:layout_constraintVertical_bias="0.434" android:textColor="#000000" android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.027"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline3" app:layout_constraintGuide_begin="4dp"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline4" app:layout_constraintGuide_end="4dp"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/guideline5" app:layout_constraintGuide_begin="264dp"/>
    <TextView
            android:text="Score :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="@+id/guideline3"
            app:layout_constraintHorizontal_bias="0.728" app:layout_constraintVertical_bias="0.238"/>
    <TextView
            android:text="Downloads :"
            android:layout_width="74dp"
            android:layout_height="18dp" android:id="@+id/textView5"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.795" app:layout_constraintVertical_bias="0.741"/>
    <TextView
            android:text="TextView"
            android:layout_width="62dp"
            android:layout_height="20dp" android:id="@+id/tv_filescore"
            android:textColor="#673AB7"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.914" app:layout_constraintVertical_bias="0.3"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/tv_filedownloads"
            android:textColor="#009688"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.971" app:layout_constraintVertical_bias="0.754"/>
</androidx.constraintlayout.widget.ConstraintLayout>